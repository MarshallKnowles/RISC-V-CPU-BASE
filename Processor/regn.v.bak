module gpr(
	input [31:0] din,
	input clk, enable, reset
	output [31:0] dout;
	)
	reg [31:0] data;
	
	//Set and retrive data
	always @(posedge clk)
		begin
		dout = data;
		
		if(enable)
			data = din;
		else
			data = data;
		
		end
	
	//asynch reset
	always @(*) begin
	
	if(reset)
		data = 32'h0000;
	else
		data = data;
		
	end
	
endmodule